	<modelVersion>4.0.0</modelVersion>
	<parent>
		<groupId>ch.unifr.diva.dip</groupId>
		<artifactId>dip-pom-osgi-repackage</artifactId>
		<version>1.0.0-SNAPSHOT</version>
		<relativePath>../pom-osgi-repackage/pom.xml</relativePath>
	</parent>

	<groupId>ch.unifr.diva.dip.libs</groupId>
	<artifactId>com.esotericsoftware.minlog</artifactId>
	<version>1.2</version>

	<name>MiniLog</name>
	<description>
		Minimal overhead Java logging.
	</description>
	<packaging>bundle</packaging>

	<properties>
		<osgi.repackage.skip>false</osgi.repackage.skip>
		<minlog.version>1.2</minlog.version>
