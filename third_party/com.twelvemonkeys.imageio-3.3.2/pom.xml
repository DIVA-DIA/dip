	<groupId>ch.unifr.diva.dip.libs</groupId>
	<artifactId>com.twelvemonkeys.imageio</artifactId>
	<!-- <version>3.0&#45;rc5</version> -->
	<version>3.3.2</version>

	<name>TwelveMonkeys ImageIO</name>
	<description>
		TwelveMonkeys ImageIO.
	</description>
	<packaging>bundle</packaging>

	<properties>
		<osgi.repackage.skip>false</osgi.repackage.skip>
		<imageio.version>3.3.2</imageio.version>
		<!-- <imageio.version>3.0&#45;rc5</imageio.version> -->
		<osgi.bundle.importPackage>
			magick;resolution:=optional,
			*
		</osgi.bundle.importPackage>
		<osgi.bundle.embedDependency>
			imageio-core,
			imageio-metadata,
			imageio-jpeg,
			imageio-tiff,
			imageio-psd,
			imageio-clippath,
			imageio-bmp,
			imageio-icns,
			imageio-pnm,
			imageio-pdf,
			imageio-pict,
			imageio-pcx,
			imageio-thumbsdb,
			imageio-sgi,
			imageio-tga,
			imageio-iff,
			imageio-hdr,
			common-lang,
			common-io,
			common-image
		</osgi.bundle.embedDependency>
	</properties>

	<dependencies>
